<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="176dp"
        android:layout_height="42dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="21dp" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="360dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="text"
        android:text="Enter Age:"
        app:layout_constraintEnd_toEndOf="@+id/button6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button4"
        tools:layout_editor_absoluteY="289dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="144dp"
        android:layout_height="57dp"
        android:layout_marginBottom="260dp"
        android:text="Proceed"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        tools:layout_editor_absoluteX="232dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="147dp"
        android:layout_height="55dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="20dp"
        android:text="Clear"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="409dp"
        android:layout_height="282dp"
        app:srcCompat="@drawable/img_1013"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="-4dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="360dp"
        android:layout_height="220dp"
        android:autoSizeMaxTextSize="16dp"
        android:autoSizeMinTextSize="12dp"
        android:autoSizeTextType="uniform"
        android:text="TextView"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="465dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="275dp"
        android:layout_height="53dp"
        android:autoSizeTextType="uniform"
        android:includeFontPadding="false"
        android:shadowColor="#050505"
        android:text="Gearhead History"
        android:textColor="#FF3D00"
        android:textColorHighlight="#D50000"
        android:textColorHint="#D50000"
        android:textColorLink="@color/design_default_color_primary_variant"
        android:textStyle="bold|italic"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="10dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="86dp"
        android:layout_height="44dp"
        android:autoSizeTextType="uniform"
        android:text="App"
        android:textColor="#FF3D00"
        android:textStyle="bold|italic"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="45dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="141dp"
        android:layout_height="58dp"
        android:text="Compare"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        tools:layout_editor_absoluteY="362dp" />

</androidx.constraintlayout.widget.ConstraintLayout>